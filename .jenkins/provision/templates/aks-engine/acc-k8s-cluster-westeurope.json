{
  "apiVersion": "vlabs",
  "properties": {
    "orchestratorProfile": {
      "orchestratorType": "Kubernetes",
      "kubernetesConfig": {
        "addons": [
          {
            "name": "tiller",
            "enabled" : true
          },
          {
            "name": "cluster-autoscaler",
            "enabled": true
          }
        ]
      }
    },
    "masterProfile": {
      "count": 1,
      "vmSize": "Standard_D2s_v3",
      "vnetSubnetId": "${VNET_SUBNET_ID}",
      "dnsPrefix": "${DNS_PREFIX}",
      "firstConsecutiveStaticIP": "192.168.0.200"
    },
    "agentPoolProfiles": [
      {
        "name": "agentswin19",
        "count": 2,
        "vmSize": "Standard_DC4s",
        "availabilityProfile": "VirtualMachineScaleSets",
        "vnetSubnetId": "${VNET_SUBNET_ID}",
        "storageProfile": "ManagedDisks",
        "osType": "Windows"
      }
    ],
    "linuxProfile": {
      "adminUsername": "azureuser",
      "ssh": {
        "publicKeys": [
          {
            "keyData": "${SSH_PUBLIC_KEY}"
          }
        ]
      }
    },
    "windowsProfile": {
      "adminUsername": "azureuser",
      "adminPassword": "${WINDOWS_ADMIN_PASSWORD}",
      "enableAutomaticUpdates": false
    },
    "servicePrincipalProfile": {
      "clientId": "f04638d3-aa98-4981-8c0e-efdd2ae6a035",
      "keyvaultSecretRef": {
        "vaultID": "/subscriptions/c4fdda6e-bfbd-4b8e-9703-037b3a45bf37/resourceGroups/osct-test-infra/providers/Microsoft.KeyVault/vaults/ostc-test-kv",
        "secretName": "sp-pwd-ostclab"
      }
    }
  }
}
